{
  "expo": {
    "name": "Tempo",
    "slug": "Temp-o-s",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/new_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Pierrofeu.tempo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/new_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Pierrofeu.tempo",
      "icon": "./assets/new_icon.png",
      "notification": {
        "icon": "./assets/ic_launcher_foreground.png",
        "color": "#7C3AED"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/new_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/new_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              "../../node_modules/@notifee/react-native/android/libs"
            ],
            "permissions": [
              "POST_NOTIFICATIONS",
              "USE_FULL_SCREEN_INTENT",
              "WAKE_LOCK",
              "FOREGROUND_SERVICE",
              "com.google.android.c2dm.permission.RECEIVE",
              "RECEIVE_BOOT_COMPLETED",
              "REQUEST_IGNORE_BATTERY_OPTIMIZATIONS",
              "SCHEDULE_EXACT_ALARM",
              "USE_EXACT_ALARM"
            ]
          },
          "ios": {  }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4eb898e4-5e4d-422d-b15d-a8717da9eff5"
      }
    }
  }
}
